{
  "name": "earth",
  "version": "1.0.0",
  "description": "Ownable earth tiles.",
  "homepage": "https://github.com/sandbar0polymer/EARTH/",
  "license": "Apache-2.0",
  "author": {
    "name": "Sandbar Polymer",
    "url": "https://github.com/sandbar0polymer"
  },
  "keywords": [
    "EARTH",
    "LAND",
    "NFT"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sandbar0polymer/EARTH.git"
  },
  "main": "index.js",
  "config": {
    "contract": "src/contract/*.sol",
    "build": "src/contract/build",
    "sk": "0xb5dc82fc5f4d82b59a38ac963a15eaaedf414f496a037bb4a52310915ac84097",
    "ganachedb": "test/ganachedb",
    "output": "dist/"
  },
  "scripts": {
    "build-solc": "solc --abi --bin $npm_package_config_contract -o $npm_package_config_build --overwrite",
    "build-typechain": "typechain --target ethers-v5 --out-dir src/contract/type $npm_package_config_build/*.abi",
    "build-contract": "npm run build-solc && npm run build-typechain",
    "build-select-network": "cp script/contracts_$npm_config_chain.json src/contracts.json",
    "build-select-network-ganache": "npm run build-select-network --chain=ganache",
    "build-webpack": "webpack --config webpack.prod.js -o $npm_package_config_output",
    "build": "npm run build-contract && npm run build-webpack",
    "start-webpack": "webpack serve --config webpack.dev.js",
    "start-ganache": "ganache --wallet.accounts $npm_package_config_sk,100000000000000000000 --database.dbPath $npm_package_config_ganachedb --miner.blockTime 15",
    "deploy-contract-ganache": "cd script && node deploy.js --nodeURL http://127.0.0.1:8545 --privateKey $npm_package_config_sk --auctionDuration 15 --outFile contracts_ganache.json",
    "clear-ganache": "rm -r $npm_package_config_ganachedb",
    "generate-metadata": "cd script && npx ts-node metadata.ts",
    "generate-images": "cd script && python metadata_image.py"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@types/yargs": "^17.0.10",
    "cesium": "^1.84.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "ethers": "^5.6.4",
    "html-webpack-plugin": "^5.3.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.8.1",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "yargs": "^17.4.1"
  }
}
