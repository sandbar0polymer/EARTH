{
  "name": "earth",
  "version": "1.0.0",
  "description": "Ownable earth tiles.",
  "homepage": "https://github.com/sandbar0polymer/EARTH/",
  "license": "Apache-2.0",
  "author": {
    "name": "Sandbar Polymer",
    "url": "https://github.com/sandbar0polymer"
  },
  "keywords": [
    "EARTH",
    "LAND",
    "NFT"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sandbar0polymer/EARTH.git"
  },
  "main": "index.js",
  "config": {
    "contract": "src/contract/*.sol",
    "build": "src/contract/build"
  },
  "scripts": {
    "build-solc": "solc --abi --bin $npm_package_config_contract -o $npm_package_config_build --overwrite",
    "build-typechain": "typechain --target ethers-v5 --out-dir src/contract/type $npm_package_config_build/*.abi",
    "build-contract": "npm run build-solc && npm run build-typechain",
    "build-webpack": "webpack --config webpack.config.js",
    "build": "npm run build-contract && npm run build-webpack",
    "start-webpack": "webpack serve --config webpack.config.js --open",
    "start": "cd script && bash ganache.sh",
    "clean-ganache": "rm -r script/db"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "cesium": "^1.84.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "ethers": "^5.6.2",
    "html-webpack-plugin": "^5.3.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.8",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
